{
  "name": "dnd-client",
  "version": "1.0.1",
  "author": {
    "name": "Jasper"
  },
  "description": "A simple program for playing DnD with friends",
  "private": true,
  "repository": {
    "url": "https://github.com/DeinAlbtraumTV/DnD-Client"
  },
  "main": "./client/index.js",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "concurrently \"sirv public --no-clear\" \"electron .\"",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "preelectron-pack": "npm run build",
    "electron-pack": "electron-builder",
    "publish": "electron-builder --win --x64 --ia32 -p always"
  },
  "build": {
    "icon": "public/favicon.png",
    "productName": "DnD Client",
    "files": [
      "public/**/*",
      "client/**/*"
    ],
    "win": {},
    "linux": {},
    "mac": {}
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@tsconfig/svelte": "^2.0.1",
    "electron": "^25.0.0",
    "electron-builder": "^24.9.1",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-polyfill-node": "^0.8.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sirv-cli": "^1.0.0",
    "socket.io-client": "^4.5.2",
    "svelte": "^3.59.1",
    "svelte-check": "^2.0.0",
    "svelte-inline-svg": "^1.1.3",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "@electron/remote": "^2.0.1",
    "cheerio": "^1.0.0-rc.10",
    "custom-electron-titlebar": "^4.2.3",
    "discord-rpc": "^4.0.1",
    "electron-updater": "^6.1.7"
  }
}
